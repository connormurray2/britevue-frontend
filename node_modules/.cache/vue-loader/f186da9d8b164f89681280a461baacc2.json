x=index;\n    this.mainReviewImageModel=this.photos[this.current_index].url;\n\n  }\n  closeModal(){\n    this.$bvModal.hide('bv-review-example');\n    this.showReviewImageModal = !this.showReviewImageModal;\n    this.$emit('changeShowReviewImageModal', true);\n  }\n}\n</script>\n"]}]}